    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import ipywidgets\n",
    "\n",
    "\n",
    "def plot(w=1):\n",
    "    X= np.random.uniform(0,5,size=100)\n",
    "    y = 2*X+w*np.random.normal(size=100)\n",
    "    plt.scatter(X,y)\n",
    "\n",
    "ipywidgets.interact(plot, w=(0,5,0.5))"
